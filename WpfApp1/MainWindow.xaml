<Window x:Class="lab2.MainWindow"
    	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    	xmlns:local="clr-namespace:lab2"
    	mc:Ignorable="d"
    	Title="MainWindow" Height="450" Width="800">
    <DockPanel LastChildFill="False">
        <TextBox DockPanel.Dock="Right" Height="321" Width="244">TextBox</TextBox>
        <TextBox DockPanel.Dock="Left" Width="224" Height="321">TextBox</TextBox>
        <Label Content="Label" DockPanel.Dock="Top" Height="91" HorizontalContentAlignment="Center"/>
        <Button DockPanel.Dock="Bottom" Content="Button" Height="39" Margin="0,0,0,100" Width="140"/>
    </DockPanel>
</Window>

